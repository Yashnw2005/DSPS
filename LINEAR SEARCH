//LINEAR SEARCH

#include<iostream>
using namespace std;
int main()
{
int a[6],n,i,target,flag=0;
cout<<"enter the size of array";
cin>>n;
for(i=0;i<n;i++)
{
cout<<"enter the element of array";
cin>>a[i];
}
cout<<"enter the target";
cin>>target;
for(i=0;i<n;i++)
{
if(target==a[i])
{
cout<<"target is present at "<<"a["<<i+1<<"]";
flag=1;

break;
}
}
if(flag==0)
{
cout<<"target is not present";
}
return 0;
}
